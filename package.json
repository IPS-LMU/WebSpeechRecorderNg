  "scripts": {
    "ng": "ng",
    "apply_module_version": "node bin/apply_version.js",
    "build_module": "npm run apply_module_version && ng build speechrecorderng",
    "pack_module": "cd dist && cd speechrecorderng && npm pack && mv speechrecorderng-*.tgz ../",
    "publish_module": "cd dist && cd speechrecorderng && rm speechrecorderng-*.tgz;npm publish",
    "start": "ng serve",
    "run_http_server": "http-server -p 8080 -c-1 dist",
    "build": "ng build",
    "build_prod": "ng build --prod -c production",
    "build_all_prod": "npm run build_module && npm run build_prod",
    "build_standalone_prod": "ng build --prod --configuration=standalone_production",
    "build_standalone_all_prod": "npm run build_module && npm run build_standalone_prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
