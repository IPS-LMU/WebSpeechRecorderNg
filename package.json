{
  "name": "web-speech-recorder-ng",
  "version": "0.0.53",
  "author": {
    "name": "Klaus JÃ¤nsch",
    "email": "klausj@phonetik.uni-muenchen.de"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "apply_version": "node bin/apply_version.js",
    "module_transpile": "npm run apply_version && ngc -p tsconfig-module-aot.json",
    "rollup_module": "rollup -c",
    "minify_module": "uglifyjs dist/module/bundles/speechrecorder.umd.js --screw-ie8 --compress --mangle --comments --output dist/module/bundles/speechrecorder.umd.min.js",
    "prepare_module_package": "node bin/copy_module_package_cfg.js",
    "build_module": "npm run module_transpile && npm run rollup_module && npm run minify_module && npm run prepare_module_package",
    "pack_module": "cd dist && cd module && npm pack && mv speechrecorderng-*.tgz ../",
    "publish_module": "cd dist && cd module && rm speechrecorderng-*.tgz;npm publish",
    "start": "ng serve",
    "build": "ng build",
    "build_devel_klausj_www": "ng build --dev --base-href='/~klausj/demos/wsr/ng_devel/dist/'",
    "install_devel_klausj_www": "rsync --delete -a dist /vdata/users_public_html/klausj/www/demos/wsr/ng_devel/ && cp htaccess_www_klausj.txt /vdata/users_public_html/klausj/www/demos/wsr/ng_devel/dist/.htaccess",
    "build_install_devel_klausj_www": "npm run build_devel_klausj_www && npm run install_devel_klausj_www",
    "build_klausj_www": "npm run apply_version && ng build --prod --base-href='/~klausj/demos/wsr/ng/dist/'",
    "install_klausj_www": "rsync --delete -a dist /vdata/users_public_html/klausj/www/demos/wsr/ng/ && cp htaccess_www_klausj.txt /vdata/users_public_html/klausj/www/demos/wsr/ng/dist/.htaccess",
    "build_install_klausj_www": "npm run build_klausj_www && npm run install_klausj_www",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.4",
    "@angular/cdk": "^6.2.1",
    "@angular/common": "^6.0.4",
    "@angular/compiler": "^6.0.4",
    "@angular/core": "^6.0.4",
    "@angular/flex-layout": "6.0.0-beta.16",
    "@angular/forms": "^6.0.4",
    "@angular/http": "^6.0.4",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "^6.0.4",
    "@angular/platform-browser-dynamic": "^6.0.4",
    "@angular/router": "^6.0.4",
    "core-js": "^2.5.1",
    "intl": "^1.2.5",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.0",
    "webpack": "^4.12.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.4",
    "@angular/language-service": "^6.0.4",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^6.0.89",
    "codelyzer": "~4.0.1",
    "hoek": "5.0.3",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "node-sass": "^4.9.2",
    "protractor": "^5.3.2",
    "rollup": "^0.45.2",
    "ts-node": "^3.3.0",
    "tslint": "~5.3.2",
    "typescript": "2.7.2",
    "uglify-js": "^2.8.29"
  }
}
