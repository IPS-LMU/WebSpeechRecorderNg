                            "texts": [
                              {
                                "type": "text",
                                "text": "„Wind und Sonne machten Wette,"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Wer die meisten Kräfte hätte,"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Einen armen Wandersmann"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Seiner Kleider zu berauben."
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Wind begann;"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Doch sein Schnauben"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Tat ihm nichts; der Wandersmann"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Zog den Mantel dichter an."
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Wind verzweifelt nun und ruht;"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Und ein lieber Sonnenschein"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Füllt mit holder, sanfter Gluth"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Wanderers Gebein."
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Hüllt er nun sich tiefer ein?"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Nein!"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Ab wirft er nun sein Gewand,"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "text",
                                "text": "Und die Sonne überwand."
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "linebreak"
                              },
                              {
                                "type": "font",
